{% extends 'front/base.html.twig' %}

{% block title %}Projet Solo{% endblock %}

{% block body %}

<div class="container">



    <div class="solo_content">

        {% for solo in solos %}
            
        <div class="solo_content_card" >
                <img class="solo_image" src="{{ asset('assets/images/' ~ solo.media) }}" alt="">
            <div class="solo_image_text">
                <p class="card-text">{{solo.title}}</p>
            </div>
        </div>

        {% endfor %}
        
    </div>

</div>



{% endblock %}